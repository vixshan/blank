{
  "name": "discord-js-bot",
  "version": "5.5.0",
  "description": "An open-source, multipurpose discord bot built using discord-js",
  "main": "src/index.js",
  "author": "Vikshan",
  "license": "ISC",
  "module": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "d": "nodemon .",
    "start": "node .",
    "f": "prettier --write src",
    "u": "git pull && npm update && npm run f",
    "start-replit": "node src/index.js",
    "build": "cd astro && npm i && npm run build",
    "prepare": "husky"
  },
  "homepage": "https://github.com/vixshan/amina#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vixshan/amina.git"
  },
  "bugs": {
    "url": "https://github.com/vixshan/amina/issues/new/choose"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.7.7",
    "btoa": "^1.2.1",
    "common-tags": "^1.8.2",
    "discord-gamecord": "^4.4.2",
    "discord-giveaways": "^6.0.1",
    "discord-together": "^1.3.31",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "enhanced-ms": "^3.0.0",
    "fixedsize-map": "^1.1.0",
    "lavalink-client": "^2.4.1",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.8.0",
    "node-fetch": "^2.7.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "pretty-ms": "^7.0.1",
    "sourcebin_js": "^0.0.3-ignore",
    "string-progressbar": "^1.0.4",
    "table": "^6.8.2"
